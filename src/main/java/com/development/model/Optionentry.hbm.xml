<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.development.model">
    <class name="Optionentry" table="optionentry">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="value_string" column="value_string" />
        <many-to-one name="Optionset" class="com.development.model.Optionset"
            column="option_set_id" unique="true" not-null="true"
            cascade="all" />
    </class> 
</hibernate-mapping>