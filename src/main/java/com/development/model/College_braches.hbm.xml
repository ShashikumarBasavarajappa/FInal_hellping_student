<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.development.model">
    <class name="College_braches" table="College_braches">
        <id name="id" column="ID">
            <generator class="increment"/>
        </id>
        <property name="college_id" column="college_id" />
        <property name="branch_name" column="branch_name" />
        
    </class> 
</hibernate-mapping>