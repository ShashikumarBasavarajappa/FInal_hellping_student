<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.development.model">
    <class name="SearchEngine" table="Registration">
        <id name="id" column="USER_ID">
            <generator class="native"/>
        </id>
        <property name="first_name" column="first_name" />
        <property name="last_name" column="last_name" />
        <property name="email" column="email" />
        <property name="password" column="PASSWORD" />
        <property name="is_archived" column="is_archived" />
    </class> 
</hibernate-mapping>