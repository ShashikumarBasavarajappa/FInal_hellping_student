<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.development.model">
    <class name="Lecture" table="lecture">
        <id name="id" column="ID">
            <generator class="increment"/>
        </id>
        <property name="college_id" column="college_id" />
        <property name="name" column="name" />
        <property name="branch" column="branch" />
        <property name="active_lecture" column="active_lecture" />  
    </class> 
</hibernate-mapping>