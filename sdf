<!-- for the Courses -->

insert into courses (program_id,title,course_number,department,credits,credit_hours,semester_quarter_trimester_system_id,required_id,course_env_oncampus_id,
is_consulting_project_id,session_winter_id,session_summer_id,session_spring_id,session_fall_id,includes_field_experience_id,is_related_to_residency_or_internship_id,comments,
updated_on,course_env_online_id,attributes,cycle_id)

select 335,title,course_number,department,credits,credit_hours,semester_quarter_trimester_system_id,required_id,course_env_oncampus_id,
is_consulting_project_id,session_winter_id,session_summer_id,session_spring_id,session_fall_id,includes_field_experience_id,is_related_to_residency_or_internship_id,comments,
updated_on,course_env_online_id,attributes,cycle_id from courses where program_id=489


<!-- to the Competency Analusis -->

insert into competency_mapping (program_id,competency_id,attributes,cycle_id) 

select 335,competency_id,attributes,cycle_id from competency_mapping where program_id=489



